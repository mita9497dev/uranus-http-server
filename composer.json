{
    "name": "mita/uranus-http-server",
    "description": "A small and flexible PHP HTTP server framework based on Slim 4",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mita\\UranusHttpServer\\": "src/", 
            "Mita\\UranusHttpServerExamples\\": "examples/"
        }
    },
    "authors": [
        {
            "name": "mita9497dev",
            "email": "mita9497dev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true, 
    "bin": [
        "uranus"
    ],
    "scripts": {
        "post-install-cmd": [
            "Mita\\UranusHttpServer\\Console\\Setup::createUranusFile"
        ], 
        "uranus": "php vendor/bin/uranus"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "slim/slim": "4.14",
        "php-di/php-di": "^6.4",
        "illuminate/database": "8.12",
        "robmorgan/phinx": "^0.14.0",
        "monolog/monolog": "^2.9",
        "sentry/sentry": "^4.9",
        "slim/twig-view": "^3.4",
        "bryanjhv/slim-session": "~4.0",
        "symfony/console": "^5.4",
        "vlucas/phpdotenv": "^5.6",
        "filp/whoops": "^2.15",
        "symfony/cache": "^5.4",
        "respect/validation": "^2.2",
        "symfony/finder": "^5.4",
        "league/fractal": "^0.20.1",
        "lcobucci/jwt": "^4.3",
        "zircote/swagger-php": "^4.10",
        "doctrine/annotations": "^2.0",
        "php-amqplib/php-amqplib": "^3.7",
        "react/event-loop": "^1.5",
        "league/flysystem": "^2.5"
    }, 
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "mockery/mockery": "^1.4",
        "fakerphp/faker": "^1.9"
    },
    "version": "1.0.1"
}
